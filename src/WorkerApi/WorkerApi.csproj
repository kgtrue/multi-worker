<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="10.0.0"/>
        <PackageReference Include="TickerQ" Version="10.0.0-beta.16" />
        <PackageReference Include="TickerQ.Caching.StackExchangeRedis" Version="10.0.0-beta.16" />
        <PackageReference Include="TickerQ.Dashboard" Version="10.0.0-beta.16" />
        <PackageReference Include="TickerQ.EntityFrameworkCore" Version="10.0.0-beta.16" />
        <PackageReference Include="TickerQ.Instrumentation.OpenTelemetry" Version="10.0.0-beta.16" />
    </ItemGroup>

    <ItemGroup>
      <Content Include="..\..\.dockerignore">
        <Link>.dockerignore</Link>
      </Content>
    </ItemGroup>

</Project>
